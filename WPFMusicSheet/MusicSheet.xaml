<UserControl x:Class="DotNetMusic.WPF.MusicSheet"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:wpf="clr-namespace:AlphaTab.Platform.CSharp.Wpf;assembly=AlphaTab">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/AlphaTab;component/AlphaTab.CSharp/Platform/CSharp/Wpf/Templates.xaml"/>
                <ResourceDictionary>
                    <Style TargetType="Button" x:Key="ToolbarButtonStyle">
                        <Style.Triggers>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Opacity" Value="0.5" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Image Name="imagi" HorizontalAlignment="Left"></Image>      
    </Grid>
</UserControl>
